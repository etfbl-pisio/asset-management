{
  "rules": [
    {
      "methods": ["GET"],
      "pattern": "/state",
      "roles": ["ADMIN","USER"]
    },
    {
      "methods": ["GET"],
      "pattern": "/asset-types",
      "roles": [
        "ADMIN","USER"
      ]
    },
    {
      "methods": ["GET"],
      "pattern": "/asset-statuses",
      "roles": [
        "ADMIN","USER"
      ]
    },
    {
      "methods": [],
      "pattern": "/asset-statuses/**",
      "roles": [
        "ADMIN"
      ]
    },
    {
      "methods": [],
      "pattern": "/asset-types/**",
      "roles": [
        "ADMIN"
      ]
    },
    {
      "methods": [],
      "pattern": "/assets/**",
      "roles": [
        "USER","ADMIN"
      ]
    },
    {
      "methods": [],
      "pattern": "/locations/**",
      "roles": [
        "USER","ADMIN"
      ]
    },
    {
      "methods": ["PUT"],
      "pattern": "/users/*",
      "roles": [
        "ADMIN","USER"
      ]
    },
    {
      "methods": [],
      "pattern": "/users/**",
      "roles": [
        "ADMIN"
      ]
    }
  ]
}